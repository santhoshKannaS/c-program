#include <stdio.h>
#include <conio.h>
#define MAX_SIZE 100
int main()

{
    int arr[MAX_SIZE];
    int n,i;
    printf("Enter a number of array : ");
    scanf("%d", &n);
    
    printf("Enter a elements: ");
    for(i=0;i<n;i++)
    {
    scanf("%d", arr[i]);
    }
   printf("The reversed array is: ");
   for(i=n-1;i>=0;i--)
   {
      scanf("%d\t", arr[i]);
   }
    
    return 0;
}
