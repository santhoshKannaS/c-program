#include<stdio.h>
#include<conio.h>
int main()
{
    int i,n;
    printf("Enter a numbers: ");
    for(i=0;i<n;i++){
    scanf("%d", &n);
    }
    for(i=0;i>10;i++){
        if(i != n){
            printf("0");
        }
        else{
            printf("n");
            }
        }
    return 0;
}
